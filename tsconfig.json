{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "removeComments": false,
    "noImplicitAny": true,
    "declaration": true,
    "module": "commonjs",
    "outDir": "transpiled",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"]
  },
  "include": ["x"],
  "exclude": ["transpiled", "node_modules", "test"]
}
